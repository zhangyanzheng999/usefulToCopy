<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bosssoft.hr.train.dao.StudentMapper">
    <!--开启二级缓存-->
    <cache/>
    <select id="getStudentById" resultType="com.bosssoft.hr.train.bean.Student">
        select *
        from tb_student
        where id = #{id}
    </select>

    <insert id="insertStudent" useGeneratedKeys="true" keyProperty="id">
        insert into tb_student(score)
        values (#{score})
    </insert>


</mapper>   