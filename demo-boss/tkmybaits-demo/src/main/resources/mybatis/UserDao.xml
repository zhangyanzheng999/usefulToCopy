<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.bosssoft.hr.train.dao.mapper.UserDao">


    <select id="findUsersLikeName" resultType="com.bosssoft.hr.train.pojo.entity.User">
        SELECT *
        FROM t_user
        WHERE name LIKE #{name}
    </select>
</mapper>